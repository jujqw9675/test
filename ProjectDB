create table Teacher(
	name varchar(20),
	birth varchar(20),
	id varchar(10),
	pass varchar(20),
	teachnum varchar(20)
);

create table Student(
	num varchar(10),
	name varchar(20),
	phone varchar(30),
	addr varchar(100),
	korean number,
	english number,
	math number,
	java number,
	android number,
	all_score number,
	avg_score number
);

select * from Teacher;

select * from Student;

drop table student;

drop table Teacher;
